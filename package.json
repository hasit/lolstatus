{
  "name": "lolstatus",
  "productName": "LoL Status",
  "version": "1.0.0",
  "description": "LoL status in your menubar",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
    "build:macos": "electron-packager . \"LoL Status\" --overwrite --out=dist/osx --platform=darwin --arch=x64 --icon=public/icons/icon.icns --prune --ignore=dist --ignore=app --ignore=assets --ignore=README.md --ignore=.gitignore --ignore=.babelrc",
    "build:win": "electron-packager . \"LoL Status\" --overwrite --out=dist/win --platform=win32 --arch=x64 --icon=public/icons/win/icon.ico --prune --ignore=dist --ignore=app --ignore=assets --ignore=README.md --ignore=.gitignore --ignore=.babelrc"
  },
  "author": {
    "name": "Hasit Mistry",
    "email": "hasitnm@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasit/lolstatus"
  },
  "bugs": {
    "url": "https://github.com/hasit/lolstatus/issues"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "classnames": "^2.2.3",
    "electron-positioner": "^3.0.0",
    "electron-prebuilt": "^0.36.0",
    "electron-reload": "^0.2.0",
    "jquery": "^2.2.3",
    "open": "0.0.5",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-dom": "^0.14.7",
    "react-select": "^1.0.0-rc.2"
  }
}
